    "clean-end": "npx run-func scripts/cleaner.js cleanEnd",
    "construct-tool": "npm run bundle && npm run zip",
    "bundle": "npx run-func scripts/tool-bundler.js bundle",
    "zip": "npx run-func scripts/tool-zipper.js zip"
  },
